<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:orientation="horizontal"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="0dp" android:layout_weight="1"
        android:layout_height="@dimen/schedule_detail_layout_height" android:text="@string/meeting_member"
        android:gravity="center_vertical" android:paddingLeft="@dimen/scheduledetail_layout_padding"
        android:textColor="@color/black"/>
    <LinearLayout
        android:layout_width="0dp" android:layout_weight="3"
        android:layout_height="wrap_content" android:orientation="vertical"
        android:background="@color/stroken_color">
        <RelativeLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="@dimen/schedule_detail_layout_height" android:visibility="gone"
            android:background="@color/white" android:gravity="center_vertical"
            android:id="@+id/member_one_layout">
            <TextView
                android:id="@+id/member_one_name" android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/member_one_phone" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/scheduledetail_layout_padding"/>
        </RelativeLayout>
        

        <RelativeLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="@dimen/schedule_detail_layout_height" android:visibility="gone"
            android:layout_marginTop="@dimen/stroken_height" android:gravity="center_vertical"
            android:background="@color/white" android:id="@+id/member_two_layout">
            <TextView
                android:id="@+id/member_two_name" android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/member_two_phone" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/scheduledetail_layout_padding" />
        </RelativeLayout>
        

        <RelativeLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="@dimen/schedule_detail_layout_height" android:visibility="gone"
            android:layout_marginTop="@dimen/stroken_height" android:gravity="center_vertical"
            android:background="@color/white" android:id="@+id/member_three_layout">
            <TextView
                android:id="@+id/member_three_name" android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/member_three_phone" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/scheduledetail_layout_padding" />
        </RelativeLayout>
        

        <RelativeLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="@dimen/schedule_detail_layout_height" android:visibility="gone"
            android:layout_marginTop="@dimen/stroken_height" android:gravity="center_vertical"
            android:background="@color/white" android:id="@+id/member_four_layout">
            <TextView
                android:id="@+id/member_four_name" android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/member_four_phone" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/scheduledetail_layout_padding" />
        </RelativeLayout>
        

        <RelativeLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="@dimen/schedule_detail_layout_height" android:visibility="gone"
            android:layout_marginTop="@dimen/stroken_height" android:gravity="center_vertical"
            android:background="@color/white" android:id="@+id/member_five_layout">
            <TextView
                android:id="@+id/member_five_name" android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/member_five_phone" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/scheduledetail_layout_padding" />
        </RelativeLayout>
        

        <RelativeLayout
            android:layout_width="match_parent" android:orientation="horizontal"
            android:layout_height="@dimen/schedule_detail_layout_height" android:visibility="gone"
            android:layout_marginTop="@dimen/stroken_height" android:gravity="center_vertical"
            android:background="@color/white" android:id="@+id/member_six_layout">
            <TextView
                android:id="@+id/member_six_name" android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/member_six_phone" android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/scheduledetail_layout_padding" />
        </RelativeLayout>
        
    </LinearLayout>
</LinearLayout>
